.pop-up{
  width:250px;
  height:250px;
  background:rgba(255,255,255,1);
  border-radius:12px;
  text-align: center;
}
.pop-up image{
    width: 80px;
    height: 65px;
    margin: 25px auto 0;
}
.pop_up1 {
    font-size: 15px;
    color:#2A2A2A;
}
.pop_up2 {
    font-size: 12px;
    color:#8F8F8F;
    margin-top: 10px;
}
.pop_up3 {
    font-size: 12px;
    color:#8F8F8F;
}
.back {
    width: 50%;
    background: #fff;
    border: 1px solid #FD9827;
    margin: 25px auto;
    color: #FD9827;
    height: 37px;
    font-size:13px;
    line-height: 37px;
}
navigator:hover{
  background: #fff;
}

text:hover{
  background: #fff;
}